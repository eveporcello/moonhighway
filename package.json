{
  "name": "moonhighway",
  "version": "0.0.3",
  "description": "Moon Highway | Instructor-Led Training for Software Engineers",
  "main": "./src/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "prestart": "./node_modules/.bin/webpack",
    "start": "./node_modules/.bin/httpster -d ./dist/ -p 3000",
    "dev": "open http://localhost:3000 & ./node_modules/.bin/webpack-dev-server",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --require ./test/setup.js --require ./node_modules/ignore-styles --recursive",
    "shallow-test": "mocha --compilers js:babel-core/register --require ./test/setup.js --require ./node_modules/ignore-styles --recursive",
    "posttest": "npm run lint",
    "coverage": "open ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eveporcello/moonhighway.git"
  },
  "keywords": [
    "React",
    "tutorial",
    "pictures",
    "drag",
    "and",
    "drop"
  ],
  "author": "Alex Banks <alex@moonhighway.com> (http://www.moonhighway.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MoonTahoe/rowdy-pictures/issues"
  },
  "homepage": "http://staging-moonhighway.herokuapp.com",
  "dependencies": {
    "httpster": "^1.0.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "^1.0.0-rc-7",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.0",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.9.0",
    "ignore-styles": "^4.0.0",
    "isparta": "^4.0.0",
    "istanbul": "0.4.4",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-stateless-wrapper": "^1.0.7",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
